{
  "version": 2,
  "builds": [
    {
      "src": "/index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/logo",
      "dest": "/img/logo.svg"
    },
    {
      "src": "/manifest.json",
      "dest": "/manifest.json"
    },
    {
      "src": "/(.*)",
      "headers": {
        "cache-control": "s-maxage=30"
      },
      "dest": "/index.html"
    }
  ]
}
